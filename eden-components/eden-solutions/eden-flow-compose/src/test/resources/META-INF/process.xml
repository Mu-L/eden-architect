<?xml version="1.0" encoding="UTF-8"?>
<context xmlns="http://www.w3school.com.cn"
		 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		 xsi:schemaLocation="http://www.w3school.com.cn flow.xsd">

	<process name="order">
		<node name="createOrder" class="org.ylzl.eden.flow.compose.CreateOrderProcessor"
			  next="riskCheck"
			  begin="true"/>
		<node name="riskCheck" class="org.ylzl.eden.flow.compose.RiskCheckProcessor"
			  next="lockCoupon"/>
		<node name="lockCoupon" class="org.ylzl.eden.flow.compose.LockCouponProcessor"
			  next="deductStock"/>
		<node name="deductStock" class="org.ylzl.eden.flow.compose.DeductStockProcessor"
			  next="saveOrder"/>
		<node name="saveOrder" class="org.ylzl.eden.flow.compose.SaveOrderProcessor"/>
	</process>

</context>
