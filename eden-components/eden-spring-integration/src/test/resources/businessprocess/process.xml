<?xml version="1.0" encoding="UTF-8"?>
<context xmlns="http://www.w3school.com.cn"
		 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		 xsi:schemaLocation="http://www.w3school.com.cn business-process.xsd">

	<process name="order">
		<node name="createOrder" class="org.ylzl.eden.spring.integration.businessprocess.CreateOrderProcessor"
			  next="riskCheck"
			  begin="true"/>
		<node name="riskCheck" class="org.ylzl.eden.spring.integration.businessprocess.RiskCheckProcessor"
			  next="lockCoupon"/>
		<node name="lockCoupon" class="org.ylzl.eden.spring.integration.businessprocess.LockCouponProcessor"
			  next="deductStock"/>
		<node name="deductStock" class="org.ylzl.eden.spring.integration.businessprocess.DeductStockProcessor"
			  next="saveOrder"  />
		<node name="saveOrder" class="org.ylzl.eden.spring.integration.businessprocess.SaveOrderProcessor" />
	</process>

</context>
